<div class="container mt-5">
  <h1> Patients </h1>
  <div class="mt-5">
    <ul>
      <% @users.each do |user| %>
        <li><%= user.first_name %> <%= user.last_name %></li>
        <li class="list-unstyled mb-3"><%= link_to "See Patients Profile", user_path(user)%></li>
      <% end %>
    </ul>
  </div>
</div>
